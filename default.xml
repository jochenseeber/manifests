<manifest>
    <!-- Common Settings -->
    <remote name="origin" fetch="https://github.com/" pushurl="git@github.com:"/>
    <default revision="master" remote="origin" sync-j="4" />

    <!-- Workspace -->
    <project name="jochenseeber/workspace.git" path="workspace">
        <linkfile src="build.gradle" dest="build.gradle" />
        <linkfile src="settings.gradle" dest="settings.gradle" />
    </project>

    <!-- Gradle plugins -->
    <project name="jochenseeber/gradle-bintray-config.git" path="gradle-bintray-config" groups="gradle-plugins"/>
    <project name="jochenseeber/gradle-docker-config.git" path="gradle-docker-config" groups="gradle-plugins"/>
    <project name="jochenseeber/gradle-git-config.git" path="gradle-git-config" groups="gradle-plugins"/>
    <project name="jochenseeber/gradle-github-config.git" path="gradle-github-config" groups="gradle-plugins"/>
    <project name="jochenseeber/gradle-license-config.git" path="gradle-license-config" groups="gradle-plugins"/>
    <project name="jochenseeber/gradle-plugin-publish-config.git" path="gradle-plugin-publish-config" groups="gradle-plugins"/>
    <project name="jochenseeber/gradle-project-config.git" path="gradle-project-config" groups="gradle-plugins"/>
    <project name="jochenseeber/gradle-test-kit.git" path="gradle-test-kit" groups="gradle-plugins"/>
    <project name="jochenseeber/gradle-workspace-plugin.git" path="gradle-workspace-plugin" groups="gradle-plugins"/>
    <project name="jochenseeber/gradle-wsimport-demo.git" path="gradle-wsimport-demo" groups="gradle-plugins"/>
    <project name="jochenseeber/gradle-wsimport-plugin.git" path="gradle-wsimport-plugin" groups="gradle-plugins"/>

    <!-- Libraries -->
    <project name="jochenseeber/guice-squeezer.git" path="guice-squeezer" groups="3rdparty"/>

    <!-- Annotation projects -->
    <project name="jochenseeber/byte-buddy-annotations.git" path="byte-buddy-annotations" groups="annotations"/>
    <project name="jochenseeber/gradle-api-annotations.git" path="gradle-api-annotations" groups="annotations"/>
    <project name="jochenseeber/guice-annotations.git" path="guice-annotations" groups="annotations"/>
    <project name="jochenseeber/jdk-annotations.git" path="jdk-annotations" groups="annotations"/>
    <project name="jochenseeber/junit-annotations.git" path="junit-annotations" groups="annotations"/>
    <project name="jochenseeber/mockito-annotations.git" path="mockito-annotations" groups="annotations"/>
</manifest>
